//----------------------------------------------------------------------------
// botprofile.db
//
// This database defines bot "personalities". 
// Feel free to edit it and define your own bots.
//
//----------------------------------------------------------------------------

//
// All profiles begin with this data and overwrite their own
//
Default
	Skill = 50
	Aggression = 50
	ReactionTime = 0.4
	AttackDelay = 0.5
	Teamwork = 75
	WeaponPreference = none
	Difficulty = NORMAL
	VoicePitch = 100
End

//----------------------------------------------------------------------------

//
// These templates inherit from Default and override with their values
// The name of the template defines a type that is used by individual bot profiles
//

// weapon preference templates
Template Rifle
	WeaponPreference = akm
	WeaponPreference = m16
End

Template Sniper
	WeaponPreference = mosin
	WeaponPreference = m40a1
End

Template Power
	WeaponPreference = sks
	WeaponPreference = m14
End

Template Shotgun
	WeaponPreference = toz
	WeaponPreference = m590
End

Template Spray
	WeaponPreference = mp40
	WeaponPreference = mp5
End

// skill templates
Template Elite
	Skill = 80
	Aggression = 100
	ReactionTime = 0.25
	AttackDelay = 0.2
	Difficulty = EXPERT
End

Template Expert
	Skill = 75
	Aggression = 90
	ReactionTime = 0.3
	AttackDelay = 0.3
	Difficulty = EXPERT
End

Template VeryHard
	Skill = 60
	Aggression = 80
	ReactionTime = 0.35
	AttackDelay = 0.4
	Difficulty = HARD+EXPERT
End

Template Hard
	Skill = 50
	Aggression = 75
	ReactionTime = 0.4
	AttackDelay = 0.55
	Difficulty = HARD
End

Template Tough
	Skill = 45
	Aggression = 70
	ReactionTime = 0.45
	AttackDelay = 0.65
	Difficulty = NORMAL+HARD
End

Template Normal
	Skill = 35
	Aggression = 65
	ReactionTime = 0.5
	AttackDelay = 0.75
	Difficulty = NORMAL
End

Template Fair
	Skill = 25
	Aggression = 50
	ReactionTime = 0.55
	AttackDelay = 0.85
	Difficulty = EASY+NORMAL 
End

Template Easy
	Skill = 15
	Aggression = 25
	ReactionTime = 0.8
	AttackDelay = 0.9
	Difficulty = EASY
End

